{
  "import": [
    "@cspell/dict-lorem-ipsum/cspell-ext.json"
  ],
  "caseSensitive": false,
  "dictionaries": [
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "./.cspell/custom-words.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/lib/**",
    "**/docs/**",
    "**/stats.html"
  ]
}