{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "APP_TARGET"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "cache": true,
      "persistent": false,
      "outputs": [
        "build"
      ],
      "dependsOn": [
        "^build"
      ]
    }
  }
}