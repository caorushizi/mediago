{
  "name": "mediago",
  "version": "0.1.0",
  "description": "mediago",
  "author": "caorushizi",
  "homepage": "https://downloader.caorushizi.cn/",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "knip": "knip",
    "dev:electron": "turbo run dev:electron",
    "dev:server": "turbo run dev:server",
    "dev": "turbo run dev:all",
    "build:electron": "turbo run build:electron",
    "build:server": "turbo run build:server",
    "build": "turbo run build:all",
    "lint": "turbo run lint",
    "rebuild:server": "pnpm -F @mediago/server rebuild",
    "rebuild:electron": "pnpm -F @mediago/electron run rebuild",
    "rebuild:workspace": "pnpm rebuild:server && pnpm rebuild:electron",
    "pack:server": "turbo run pack:server",
    "pack:electron": "turbo run pack:electron",
    "release:server": "turbo run release:server",
    "release:electron": "turbo run release:electron",
    "docs:dev": "pnpm -F @mediago/docs run docs:dev",
    "docs:build": "pnpm -F @mediago/docs run docs:build"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/node": "^24.7.0",
    "knip": "^5.64.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3"
  }
}